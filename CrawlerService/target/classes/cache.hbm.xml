<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC  
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"  
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.jagan.CrawlerService.models.ScrappedUrl" table="urlsscache">
		<id name="url" column="url" type="string">
		</id>
		<property name="urlType" column="urltype" type="string" />
		<property name="retailerName" column="retailerName" type="string" />
		<property name="visited" column="visited" type="boolean" />
	</class>

</hibernate-mapping>  